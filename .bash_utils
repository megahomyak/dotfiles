alias superclippy="cargo clippy -- -W clippy::pedantic -W clippy::nursery -W clippy::all"
c() { cd "$@" && ls; }
alias rand="ls | shuf -n 1"
detach() {
    "$@" &
    disown -h %%
}
shopt -s expand_aliases
alias gc="git add --all && git commit"
alias gp="git push"
alias gcp="gc && gp"
alias gs="git status"
alias hint="chatgpt cmd"

HISTSIZE = 30000
HISTFILESIZE = 30000
